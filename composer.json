{
    "name": "hakito/php-stuzza-eps-banktransfer",
    "description": "Stuzza e-payment standard implementation for PHP",
    "keywords": ["eps", "payments", "stuzza", "bank transfer"],
    "type": "library",
    "version": "3.0.23",
    "license": "GPL-2.0",
    "homepage": "https://github.com/hakito/PHP-Stuzza-EPS-BankTransfer",
    "authors": [
        {
            "name": "Hakito",
            "homepage": "https://github.com/hakito/PHP-Stuzza-EPS-BankTransfer/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "psr/http-client": "^1.0",
        "psr/http-message": "^2.0",
        "psr/http-factory": "^1.0",
        "ext-simplexml": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "goetas-webservices/xsd2php": "^0.4.13",
        "goetas-webservices/xsd2php-runtime": "^0.2.17",
        "jms/serializer": "^3.29"
    },
    "require-dev": {
      "phpunit/phpunit": "9.*",
      "php-coveralls/php-coveralls": "^2.7",
      "nyholm/psr7": "^1.8",
      "symfony/http-client": "^5.4"
    },
    "autoload": {
      "psr-4": {
        "Externet\\EpsBankTransfer\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Externet\\EpsBankTransfer\\Tests\\": "tests/Unit/"
      }
    }
}
